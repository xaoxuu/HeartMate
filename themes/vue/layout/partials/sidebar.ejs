<div class="sidebar">
  <% if (index == true) { %>
    <div class="sidebar-inner-index">
        <ul class="main-menu">
            <br><h2><a href="<%- url_for("/") %>"><%- config.title %></a></h2>
          <%- partial('partials/main_menu', { context: 'sidebar' }) %>
        </ul>
      </div>
  <% } else { %>
    <div class="sidebar-inner">
      <% if (page.lang == "" ) { %>
        <ul class="main-menu">
           <br><h2><a href="<%- url_for("/") %>"><%- config.title %></a></h2>
          <%- partial('partials/main_menu', { context: 'sidebar' }) %>
        </ul>
      <% } %>
      <% if (type && type !== 'search') { %>
        <div class="list">
          <h2>
            <% titles = {
              api: 'API',
              examples: '示例',
              guide: '教程',
              cookbook: 'Cookbook<sup class="beta">beta</sup>',
              'style-guide': '风格指南'
            } %>
            <%- type %>
          </h2>
          <%- partial('partials/toc', { type: type }) %>
        </div>
      <% } %>
    </div>
  <% } %>
</div>
